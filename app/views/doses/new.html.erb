<h1>New Dose</h1>

<form action="/doses" method="post">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <label for="name">Description: </label>
  <input type="text" name="description[description]" id="name">
  <input type="association" name="ingredient" id=ingredient>
  <input type="submit">

</form>

